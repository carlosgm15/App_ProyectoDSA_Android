<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.upc.dsa.app_proyecto">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity_Sonido"
            android:theme="@style/ThemeSonido"/>
        <activity android:name=".Activity_Friend"
            android:theme="@style/ThemeFriends"/>
        <activity android:name=".Activity_Score"
            android:theme="@style/ThemeScore"/>
        <activity android:name=".Activity_Idioma"
            android:theme="@style/ThemeIdioma"/>
        <activity android:name=".Activity_Ajustes"
            android:theme="@style/ThemeAjustes"/>
        <activity android:name=".Activity_Play"
            android:theme="@style/ThemePlay"/>
        <activity android:name=".Dashboard">

        </activity>
        <activity android:name=".MainActivity2" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>